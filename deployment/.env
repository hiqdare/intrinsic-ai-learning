# Environment variables for intrinsic.ai learning platform

# MongoDB credentials
MONGO_USERNAME=intrinsic_admin
MONGO_PASSWORD=change_this_secure_password_in_production

# JWT and session secrets
JWT_SECRET=intrinsic_learning_jwt_secret_change_in_production
SESSION_SECRET=intrinsic_learning_session_secret_change_in_production

# Google OAuth credentials
GOOGLE_CLIENT_ID=your_google_client_id
